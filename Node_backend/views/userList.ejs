<h2 class="mb-3">Users</h2>

<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">PasswordHas</th>
      </tr>
    </thead>
    <tbody>
        <% for (let i = 0; i < users.length; i++) { %>
        <tr>
            <td><%= i %></td>
            <td><%= users[i].name %></td>
            <td><%= users[i].password %></td>
        </tr>
        <% }  %>
    </tbody>
</table>